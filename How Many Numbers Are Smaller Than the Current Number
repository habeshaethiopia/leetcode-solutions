class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
       int counter ;int[] min=new int[nums.length];

       for (int i=0;i<nums.length;i++){counter=0;
           for (int j=0;j<nums.length;j++)
           {if(nums[i]>nums[j])
                counter++;
           }  min[i]=counter;
       }
       return min;
    }
}
